      "provider_intent": "unrealspeech__stream___post",
      "provider_alias_intent": "/stream",
      "text": "Stream",
      "category": "Test to Speech",
      "method": "POST",
      "type": "API",
      "params": {},
      "custom_headers": {},
      "body": {
        "Text": {
          "type": "string",
          "text": "This is the text to be synthesized to audio. Up to 1000 characters.",
          "default": "This is a test",
          "required": true
        },
        "VoiceId": {
          "type": "enum",
          "text": "Dan: Young Male, Will: Mature Male, Scarlett: Young Female, Liv: Young Female, Amy: Mature Female",
          "enum": [
            "Scarlett",
            "Dan",
            "Liv",
            "Will",
            "Amy"
          ],
          "default": "Scarlett",
          "required": true
        },
        "Bitrate": {
          "type": "enum",
          "text": "Use lower values for low bandwidth or to reduce the transferred file size. Use higher values for higher fidelity.",
          "enum": [
            "320k",
            "256k",
            "192k"
          ],
          "default": "192k"
        },
        "Speed": {
          "type": "string",
          "text": "0.5: makes the audio 50% faster (i.e. 60-second audio becomes 42 seconds), -0.5: makes the audio 50% slower (i.e. 60-second audio becomes 90 seconds)",
          "default": "0"
        },
        "Pitch": {
          "type": "string",
          "text": "However, on the landing page, we default male voices to 0.92 as people tend to prefer lower/deeper male voices.",
          "default": "1"
        },
        "Codec": {
          "type": "enum",
          "text": "Codec",
          "enum": [
            "libmp3lame",
            "pcm_mulaw",
            "pcm_s16le"
          ],
          "default": "libmp3lame"
        },
        "Temperature": {
          "type": "number",
          "text": "minimum Temperature 0.1 maximum Temperature 0.8",
          "default": "0.25"
        }
      },
      "path": {},
      "domain_params": {
        "endpoint": {
          "type": "string"
        }
      },
      "meta": {
        "version": "v7",
