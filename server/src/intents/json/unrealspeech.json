        "rate_limit": [],
        "api_endpoint": "https://api.v7.unrealspeech.com/stream",
        "alias_endpoint": "/unrealspeech/stream",
        "api_ref": "https://docs.unrealspeech.com/reference/stream"
      },
      "auth": {
