      "category": "Signed URL",
      "method": "GET",
      "type": "Custom",
      "params": {
        "bucket": {
          "type": "string",
          "text": "User's AWS S3 bucket name",
          "required": true
        },
        "key_name": {
          "type": "string",
          "text": "File name to be signed",
          "required": true
        },
        "type": {
          "type": "enum",
          "text": "Request type",
          "enum": [
            "upload",
            "get"
          ]
        },
        "acl": {
          "type": "enum",
          "text": "ACL of the file object, only applicable for upload. Default: private",
          "enum": [
            "private",
            "public-read",
            "public-read-write",
            "authenticated-read",
            "aws-exec-read",
            "bucket-owner-read",
            "bucket-owner-full-control"
          ]
        },
        "expires_in": {
          "type": "number",
          "text": "Expire the signed url after this duration (in second). Default : 300 seconds"
        }
