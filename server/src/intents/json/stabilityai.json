    "stabilityai__v1__generation__engine_id__image-to-image___post": {
      "provider_intent": "stabilityai__v1__generation__engine_id__image-to-image___post",
      "provider_alias_intent": "/v1/generation/engine_id/image-to-image",
      "text": "Modify an image based on a text prompt",
      "category": "Generation/image-to-image",
      "method": "POST",
      "type": "API",
      "params": {
        "engine_id": {
          "type": "string",
          "text": "Engine ID",
          "required": true
        }
      },
      "custom_headers": {},
      "body": {
        "text_prompts": {
          "type": "array",
          "text": "An array of text prompts to use for generation",
          "required": true
        },
        "init_image": {
          "type": "string",
          "text": "Image used to initialize the diffusion process, in lieu of random noise",
          "required": true
        },
        "init_image_mode": {
          "type": "enum",
          "text": "Whether to use image_strength or step_schedule_* to control how much influence the init_image has on the result",
          "enum": [
            "image_strength",
            "step_schedule"
          ]
        },
        "image_strength": {
          "type": "number",
          "text": "How much influence the init_image has on the diffusion process. Values close to 1 will yield images very similar to the init_image while values close to 0 will yield images wildly different than the init_image. The behavior of this is meant to mirror DreamStudio's 'Image Strength' slider"
        },
        "step_schedule_start": {
          "type": "number",
          "text": "Skips a proportion of the start of the diffusion steps, allowing the init_image to influence the final generated image. Lower values will result in more influence from the init_image, while higher values will result in more influence from the diffusion steps. (e.g. a value of 0 would simply return you the init_image, where a value of 1 would return you a completely different image.)"
        },
        "step_schedule_end": {
          "type": "number",
          "text": "Skips a proportion of the end of the diffusion steps, allowing the init_image to influence the final generated image. Lower values will result in more influence from the init_image, while higher values will result in more influence from the diffusion steps"
        },
        "cfg_scale": {
          "type": "number",
          "text": "How strictly the diffusion process adheres to the prompt text (higher values keep your image closer to your prompt)"
        },
        "clip_guidance_preset": {
          "type": "enum",
          "text": "Clip Guidance Preset",
          "enum": [
            "FAST_BLUE",
            "FAST_GREEN",
            "NONE",
            "SIMPLE",
            "SLOW",
            "SLOWER",
            "SLOWEST"
          ]
        },
        "sampler": {
          "type": "enum",
          "text": "Which sampler to use for the diffusion process. If this value is omitted we'll automatically select an appropriate sampler for you",
          "enum": [
            "DDIM",
            "DDPM",
            "K_DPMPP_2M",
            "K_DPMPP_2S_ANCESTRAL",
            "K_DPM_2",
            "K_DPM_2_ANCESTRAL",
            "K_EULER",
            "K_EULER_ANCESTRAL",
            "K_HEUN",
            "K_LMS"
          ]
        },
        "samples": {
          "type": "number",
          "text": "Number of images to generate"
        },
        "seed": {
          "type": "number",
          "text": "Random noise seed (omit this option or use 0 for a random seed)"
        },
        "steps": {
          "type": "number",
          "text": "Number of diffusion steps to run"
        },
        "style_preset": {
          "type": "enum",
          "text": "Pass in a style preset to guide the image model towards a particular style. This list of style presets is subject to change",
          "enum": [
            "3d-model",
            "analog-film",
            "anime",
            "cinematic",
            "comic-book",
            "digital-art",
            "enhance",
            "fantasy-art",
            "isometric",
            "line-art",
            "low-poly",
            "modeling-compound",
            "neon-punk",
            "origami",
            "photographic",
            "pixel-art",
            "tile-texture"
          ]
        },
        "extras": {
          "type": "object",
          "text": "Extra parameters passed to the engine. These parameters are used for in-development or experimental features and may change without warning, so please use with caution"
        }
      },
      "path": {
        "engine_id": {
          "type": "string",
          "text": "Engine ID",
          "required": true
        }
      },
      "domain_params": {},
      "meta": {
