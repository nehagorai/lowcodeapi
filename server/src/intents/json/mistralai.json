    "mistralai__v1__fim__completions___post": {
      "provider_intent": "mistralai__v1__fim__completions___post",
      "provider_alias_intent": "/v1/fim/completions",
      "provider_proxy_intent": "/v1/fim/completions",
      "text": "Create FIM Completions",
      "category": "Chat",
      "method": "POST",
      "type": "API",
      "request_type": "API",
      "params": {},
      "path": {},
      "body": {
        "stop": {
          "text": "Stop generation if this token is detected",
          "type": "array"
        },
        "model": {
          "text": "ID of the model to use.",
          "type": "string"
        },
        "top_p": {
          "text": "Nucleus sampling, where the model considers the results of the tokens with with top_p probability mass. So 0.1 means only the tokens comprising the top 10% probability mass are considered.",
          "type": "number"
        },
        "prompt": {
          "text": "The text/code to complete",
          "type": "string",
          "required": true
        },
        "stream": {
          "enum": [
            true,
            false
          ],
          "text": "Whether to stream back partial progress. If set, tokens will be sent as data-only server-side events as they become available, with the stream terminated by a data: [DONE] message..",
          "type": "boolean"
        },
        "suffix": {
          "text": "Optional text/code that adds more context for the model",
          "type": "string"
        },
        "max_tokens": {
          "text": "The maximum number of tokens to generate in the completion.The token count of your prompt plus max_tokens cannot exceed the model's context length",
          "type": "number"
        },
        "min_tokens": {
          "text": "The minimum number of tokens to generate in the completion.",
          "type": "number"
        },
        "random_seed": {
          "text": "The seed to use for random sampling. If set, different calls will generate deterministic results",
          "type": "number"
        },
        "temperature": {
          "text": "Default: 0.7 What sampling temperature to use, between 0.0 and 1.0. Higher values like 0.8 will make the outptu more random, while lower values like 0.2 will make it more focused and deterministic.",
          "type": "number"
        }
      },
